{
  "name": "@delve-ai/taxonomy",
  "version": "0.1.0",
  "description": "TypeScript/JavaScript client for Delve Taxonomy API - AI-powered taxonomy generation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "taxonomy",
    "classification",
    "llm",
    "anthropic",
    "claude",
    "machine-learning",
    "nlp",
    "text-classification",
    "delve"
  ],
  "author": "Delve AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/delve-ai/delve-taxonomy"
  },
  "homepage": "https://github.com/delve-ai/delve-taxonomy#readme",
  "bugs": {
    "url": "https://github.com/delve-ai/delve-taxonomy/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  }
}
